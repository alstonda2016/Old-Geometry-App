<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.david.ezgeo.list3d"
    android:background="#751CE7"
    xmlns:ads="http://schemas.android.com/apk/res-auto">

    <com.google.android.gms.ads.AdView
        android:id="@+id/adView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        ads:adSize="BANNER"
        ads:adUnitId="@string/banner_ad_unit_id">
    </com.google.android.gms.ads.AdView>

    <Button
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:text="Cylinder"
        android:id="@+id/button19"
        android:background="#ff4c05"
        android:onClick="openCylinder"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="25dp"
        android:layout_marginTop="10dp" />

    <Button
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:text="Cone"
        android:id="@+id/button20"
        android:background="#ff4c05"
        android:onClick="openCone"
        android:layout_below="@+id/button23"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:textSize="30dp"
        android:layout_marginTop="10dp" />

    <Button
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:text="Sphere"
        android:id="@+id/button21"
        android:background="#ff4c05"
        android:onClick="openSphere"
        android:layout_below="@+id/button20"
        android:layout_alignLeft="@+id/button20"
        android:layout_alignStart="@+id/button20"
        android:textSize="30dp"
        android:layout_marginTop="10dp" />

    <Button
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:text="Rectangular Prism"
        android:id="@+id/button22"
        android:background="#ff4c05"
        android:onClick="openRecPris"
        android:layout_below="@+id/button19"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="18dp"
        android:layout_marginTop="10dp"
        android:textAlignment="center" />

    <Button
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:text="Cube"
        android:id="@+id/button23"
        android:background="#ff4c05"
        android:onClick="openCube"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/button20"
        android:layout_alignStart="@+id/button20"
        android:textSize="30dp"
        android:layout_marginTop="10dp" />

    <Button
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:text="Pyramid"
        android:id="@+id/button24"
        android:background="#ff4c05"
        android:onClick="openPyramid"
        android:layout_below="@+id/button20"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="30dp"
        android:layout_marginTop="10dp" />
</RelativeLayout>
